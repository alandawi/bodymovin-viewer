{
  "name": "bodymovin-vueviewer",
  "version": "1.0.0",
  "description": "An internal app to see all your bodymovin files in one place.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run generate && npm run server",
