    "watch": "nodemon --watch public/assets --exec npm start"
  },
  "keywords": [
    "bodymovin",
    "assets",
    "viewer",
    "vue",
    "after",
    "effects"
  ],
  "author": "Alan G. Dawidowicz",
  "license": "MIT",
  "dependencies": {
    "http-server": "^0.11.1",
    "nodemon": "^1.18.4",
    "walk": "^2.3.14"
  }
}
